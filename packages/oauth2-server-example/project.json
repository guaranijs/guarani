{
  "name": "oauth2-server-example",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/oauth2-server-example/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/oauth2-server-example",
        "main": "packages/oauth2-server-example/src/main.ts",
        "tsConfig": "packages/oauth2-server-example/tsconfig.app.json",
        "assets": [
          "packages/oauth2-server-example/src/assets",
          "packages/oauth2-server-example/src/views",
          "packages/oauth2-server-example/src/.env"
        ],
        "webpackConfig": "packages/oauth2-server-example/webpack.config.js",
        "babelUpwardRootMode": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "oauth2-server-example:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "oauth2-server-example:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/oauth2-server-example/**/*.ts"]
      }
    },
    "migration:create": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node ../../node_modules/typeorm/cli.js migration:create db/migrations/{args.name}",
        "cwd": "packages/oauth2-server-example/src"
      }
    },
    "migration:run": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node ../../../node_modules/typeorm/cli.js migration:run --data-source data-source.migration.ts",
        "cwd": "packages/oauth2-server-example/src"
      }
    },
    "migration:revert": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node ../../../node_modules/typeorm/cli.js migration:revert --data-source data-source.migration.ts",
        "cwd": "packages/oauth2-server-example/src"
      }
    },
    "seed:config": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node ./node_modules/@paranode/typeorm-seeding/cli.js config -d ./packages/oauth2-server-example/src/data-source.migration.ts",
        "cwd": ""
      }
    },
    "seed:create": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node ./node_modules/@paranode/typeorm-seeding/cli.js create -d ./packages/oauth2-server-example/src/data-source.migration.ts -f {args.name}",
        "cwd": ""
      }
    },
    "seed:run": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node --project tsconfig.base.json -r tsconfig-paths/register ./node_modules/@paranode/typeorm-seeding/cli.js seed -d ./packages/oauth2-server-example/src/data-source.migration.ts",
        "cwd": ""
      }
    }
  },
  "tags": []
}
